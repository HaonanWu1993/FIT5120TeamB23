            function checkInt(n,min,max){
                if(n<min && n>0){
                    alert("The value you entered is too small.\n" +
                        "Please input the correct value between 50-220.")
                    var dbp=document.getElementById("dbp");
                    dbp.style.backgroundColor="rgba(223,36,29,0.67)";
                }else if(n>max){
                    alert("The value you entered is too large. \n" +
                        "Please input the correct value between 50-220.\n")
                    var dbp=document.getElementById("dbp");
                    dbp.style.backgroundColor="rgba(223,36,29,0.67)";
                }else {
                    var dbp=document.getElementById("dbp");
                    dbp.style.backgroundColor="rgba(255,252,249,0)";
                }
            }
            function checkInt1(n,min,max){
                if(n<min && n>0){
                    alert("The value you entered is too small. \n" +
                        "Please input the correct value between 30-130.\n")
                    var sbp=document.getElementById("sbp");
                    sbp.style.backgroundColor="rgba(223,36,29,0.67)";
                }else if(n>max){
                    alert("The value you entered is too large. \n" +
                        "Please input the correct value between 30-130.\n")
                    var sbp=document.getElementById("sbp");
                    sbp.style.backgroundColor="rgba(223,36,29,0.67)";
                }else {
                    var sbp=document.getElementById("sbp");
                    sbp.style.backgroundColor="rgba(255, 252, 249, 0.0)";
                }
            }

            function checkInt2(n,min,max){
                if(n<0){
                    alert("You can not eat negative salt!")
                    var ysalt=document.getElementById("ysalt");
                    ysalt.style.backgroundColor="rgba(223,36,29,0.67)";
                } else if(n<min && n>0){
                    alert(" The value you entered is too small. \n" +
                        "According to recommended value of the world health organization, you should consume 5000mg of salt a day.\n")
                    var ysalt=document.getElementById("ysalt");
                    ysalt.style.backgroundColor="rgba(223,36,29,0.67)";
                }else if(n>max){
                    alert("The value you entered is too large. \n" +
                        "According to recommended value of the world health organization, you should consume 5000mg of salt a day.\n")
                    var ysalt=document.getElementById("ysalt");
                    ysalt.style.backgroundColor="rgba(223,36,29,0.67)";
                }else {
                    var ysalt=document.getElementById("ysalt");
                    ysalt.style.backgroundColor="rgba(255, 252, 249, 0.0)";
                }
            }
