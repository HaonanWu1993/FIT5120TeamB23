            function getmes() { //PDF
                html2canvas(document.getElementById('result'),{
                    onrendered:function(canvas){
                        var contentWidth = canvas.width;
                        var contentHeight = canvas.height;
                        var pageHeight = contentWidth / 592.28 * 841.89;
                        var leftHeight = contentHeight;
                        var position = 0;
                        var imgWidth = 595.28;
                        var imgHeight = 592.28/contentWidth * contentHeight;
                        var pageData = canvas.toDataURL('image/jpeg', 1.0);
                        var pdf = new jsPDF('', 'pt', 'a4');
                        if (leftHeight < pageHeight) {
                            pdf.addImage(pageData, 'JPEG', 0, 0, imgWidth, imgHeight );
                        } else {
                            while(leftHeight > 0) {
                                pdf.addImage(pageData, 'JPEG', 0, position, imgWidth, imgHeight);
                                leftHeight -= pageHeight;
                                position -= 841.89;
                                if(leftHeight > 0) {
                                    pdf.addPage();
                                }
                            }
                        }

                        pdf.save('Your report.pdf');
                    }
                });

            }
            function calculate1() {
                var dbp = document.getElementById("dbp").value;
                var sbp = document.getElementById("sbp").value;
                var ysalt=document.getElementById("ysalt").value;
                var gycm =dbp-140;
                var dycm =sbp-80;
                if (dbp==""||sbp==""||ysalt==""){
                    alert("Please input the complete data to calculate the salt level")
                } else if(dbp>220||dbp<50||sbp>130||sbp<30||ysalt<5000||ysalt>30000){
                    alert("There is something wrong with the value you entered, please check and try again.")
                } else {
                    if ((gycm > 0 || dycm > 0) && gycm >= dycm) {
                        var res = Math.max(Number(ysalt) - Number(gycm) * 100, 1);
                        var sug = "Unfortunately, you have high blood pressure.";
                        var other = "Your Blood pressure is High. You are consuming high amount of salt in your diet. This is the case of Hypertension and is danger because it makes heart pump severer to pump blood out to the body, which can cause reinforcing of the arteries, or atherosclerosis, to stroke, kidney disease, and to heart failure.\n" +
                            "</br>" +
                            "You should follow healthy lifestyle to balance your blood pressure because it may risk your life." +
                            "</br>" +
                            "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                            "2.Minimise the salt level in your diet.\n" + "</br>" +
                            "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                            "4.Limiting alcoholic beverages and smoking.\n" + "</br>" +
                            "5.Try managing your stress level.\n"+ "</br>"+"</br>";
                    } else if ((gycm > 0 || dycm > 0) && gycm <= dycm) {
                        var res = Math.max(Number(ysalt) - Number(dycm) * 100, 1);
                        var sug = "Unfortunately, you have high blood pressure.";
                        var other = "Your Blood pressure is High. You are consuming high amount of salt in your diet. This is the case of Hypertension and is danger because it makes heart pump severer to pump blood out to the body, which can cause reinforcing of the arteries, or atherosclerosis, to stroke, kidney disease, and to heart failure.\n" +
                            "</br>" +
                            "You should follow healthy lifestyle to balance your blood pressure because it may risk your life." +
                            "</br>" +
                            "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                            "2.Minimise the salt level in your diet.\n" + "</br>" +
                            "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                            "4.Limiting alcoholic beverages and smoking.\n" + "</br>" +
                            "5.Try managing your stress level.\n" + "</br>" + "</br>";
                    } else {
                        var res = 5000;
                        var sug = "Congratulations, you are in good health!";
                        var other = other = "Your blood pressure is relatively Good.You only need to take the daily intake of 5000mg of salt recommended by the World Health Organization." +
                            "</br>" +
                            "Your lifestyle is very well balanced, and you can continue doing the same with following few healthy factors.\n" + "</br>" +
                            "1.Maintaining the salt level in your diet.\n" + "</br>" +
                            "2.Limiting alcoholic beverages.\n" + "</br>" +
                            "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                            "4.Eating more healthy food to lower the risk of high blood pressure.\n" + "</br>" + "</br>"
                    }


                    document.getElementById("feedback").innerHTML = "<p style=\'font-family: Arial;text-align: center;font-size: 20px\'>Your report</p>" + "<br/>" +
                        "<p style='text-align: left;font-size: 17px'>Your BP situation:   " + sug + "<br/>" +
                        "<p style='text-align: left'>Your Diastolic blood pressure of today：  " + dbp + " mm Hg" + "<br/>" +
                        "<p style='text-align: left'>Systolic blood pressure of today：  " + sbp + " mm Hg" + "<br/>" +
                        "<p style='text-align: left'>The salt you ate yesterday：  " + ysalt + " mg" + "<br/>" +
                        "<p style='text-align: left'>Suggest salt intake for today:  " + res + " mg" + "<br/>" +
                        "<p style='text-align: left'> Other suggestion:" + other;


                }
            }

