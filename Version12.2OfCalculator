        <script>
            //This part will include all the JavaScript functions needed to interact with the user in our calculator page
            function getmes() {
                //This method is to convert the report generated by the calculator to PDF format and enable the user to download it to the local
                html2canvas(document.getElementById('result'),{
                    onrendered:function(canvas){
                        dpi:190;
                        var contentWidth = canvas.width;
                        var contentHeight = canvas.height;
                        var pageHeight = contentWidth / 592.28 * 841.89;
                        var leftHeight = contentHeight;
                        var position = 0;
                        var imgWidth = 595.28;//Set the PDF size
                        var imgHeight = 592.28/contentWidth * contentHeight;
                        var pageData = canvas.toDataURL('image/jpeg', 1.0);
                        var pdf = new jsPDF('', 'pt', 'a4');
                        if (leftHeight < pageHeight) {
                            pdf.addImage(pageData, 'JPEG', 0, 0, imgWidth, imgHeight );
                        } else {
                            while(leftHeight > 0) {
                                pdf.addImage(pageData, 'JPEG', 0, position, imgWidth, imgHeight);
                                leftHeight -= pageHeight;
                                position -= 841.89;
                                if(leftHeight > 0) {
                                    pdf.addPage();
                                }
                            }
                        }

                        pdf.save('Your report.pdf');//Use 'Your report as a document name to save'
                    },
                    background: "#fff"//Set the PDF background color;
                });

            }
            function calculate1() {
                //This function is the logical core of our entire calculator, which contains extremely rich calculation types
                var dbp = document.getElementById("dbp").value; //dbp means Diastolic blood pressure
                var sbp = document.getElementById("sbp").value;//hbp means Systolic blood pressure
                var ysalt = document.getElementById("ysalt").value;
                var gycm = dbp - 140;//140 as the upper limit of the normal value of diastolic blood pressure
                var dycm = sbp - 80;//80 as the upper limit of the normal value of systolic blood pressure
                if (dbp == "" || sbp == "" || ysalt == "") {
                    alert("Please input the complete data to calculate the salt level")//If the user input is empty, it cannot be calculated and requires the user to re-enter
                } else if (dbp > 250 || dbp < 50 || sbp > 180 || sbp < 30 || ysalt > 30000) { //If the value entered by the user exceeds the reasonable range of the calculator, the user will also be required to re-enter
                    alert("There is something wrong with the value you entered, please check and try again.")
                } else {
                    if ((gycm > 0 || dycm > 0) && gycm >= dycm && ysalt < 5000) {
                        if (Number(ysalt) - Number(gycm) * 100 > 100) {
                            //When the user has a high diastolic blood pressure and the salt intake does not exceed a reasonable range
                            var res = Math.max(Number(ysalt) - Number(gycm) * 100, 1);
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "Your Blood pressure is High. You are consuming high amount of salt in your diet. This is the case of Hypertension and is danger because it makes heart pump severer to pump blood out to the body, which can cause reinforcing of the arteries, or atherosclerosis, to stroke, kidney disease, and to heart failure.\n" +
                                "</br>" +
                                "You should follow healthy lifestyle to balance your blood pressure because it may risk your life." +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.Minimise the salt level in your diet.\n" + "</br>" +
                                "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "4.Limiting alcoholic beverages and smoking.\n" + "</br>";
                        } else {
                            var res = "Less than"+ysalt;
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "You have high blood pressure problems, but we think your salt intake is already at a low level, you can try to control your high blood pressure in other ways.\n" +
                                "</br>" +
                                "Here are also some useful suggestions for you:" +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "3.Limiting alcoholic beverages and smoking.\n" + "</br>" +
                                "4.Try managing your stress level.\n" + "</br>" + "</br>";
                        }
                    } else if ((gycm > 0 || dycm > 0) && gycm >= dycm && ysalt >= 5000) {
                        if (Number(ysalt) - Number(gycm) * 100 > 100) {
                             //When the user has a high diastolic blood pressure and the salt intake is far beyond a reasonable range
                            var res = Math.max(Number(ysalt) - Number(gycm) * 100, 1);
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "Your Blood pressure is High. You are consuming high amount of salt in your diet. This is the case of Hypertension and is danger because it makes heart pump severer to pump blood out to the body, which can cause reinforcing of the arteries, or atherosclerosis, to stroke, kidney disease, and to heart failure.\n" +
                                "</br>" +
                                "You should follow healthy lifestyle to balance your blood pressure because it may risk your life." +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.Minimise the salt level in your diet.\n" + "</br>" +
                                "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "4.Limiting alcoholic beverages and smoking.\n" + "</br>";
                        } else {
                            var res = "less than 5000";
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "Your blood pressure is too high and you need to seek medical help immediately. High blood pressure will directly threaten your life, and you should also control your salt intake, because according to the recommendations of the World Health Organization, the daily salt intake of adults should be less than 5000mg.\n" +
                                "</br>" +
                                "Here are also some useful suggestions for you:" +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.Minimise the salt level in your diet.\n" + "</br>" +
                                "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "4.Limiting alcoholic beverages and smoking.\n" + "</br>";
                        }
                    } else if ((gycm > 0 || dycm > 0) && dycm >= gycm && ysalt < 5000) {
                        if (Number(ysalt) - Number(dycm) * 100 > 100) {
                               //When the user has a high systolic blood pressure but the salt intake is still in a reasonable range
                            var res = Math.max(Number(ysalt) - Number(gycm) * 100, 1);
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "Your Blood pressure is High. You are consuming high amount of salt in your diet. This is the case of Hypertension and is danger because it makes heart pump severer to pump blood out to the body, which can cause reinforcing of the arteries, or atherosclerosis, to stroke, kidney disease, and to heart failure.\n" +
                                "</br>" +
                                "You should follow healthy lifestyle to balance your blood pressure because it may risk your life." +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.Minimise the salt level in your diet.\n" + "</br>" +
                                "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "4.Limiting alcoholic beverages and smoking.\n" + "</br>" ;
                        } else {
                            var res = "Less than"+ysalt;
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "You have high blood pressure problems, but we think your salt intake is already at a low level, you can try to control your high blood pressure in other ways.\n" +
                                "</br>" +
                                "Here are also some useful suggestions for you:" +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "3.Limiting alcoholic beverages and smoking.\n" + "</br>" +
                                "4.Try managing your stress level.\n" + "</br>" + "</br>";
                        }
                    } else if ((gycm > 0 || dycm > 0) && dycm >= gycm && ysalt >= 5000) {
                        if (Number(ysalt) - Number(dycm) * 100 > 100) {
                            //When the user has a high systolic blood pressure and the salt intake is far beyond a reasonable range
                            var res = Math.max(Number(ysalt) - Number(dycm) * 100, 1);
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "Your Blood pressure is High. You are consuming high amount of salt in your diet. This is the case of Hypertension and is danger because it makes heart pump severer to pump blood out to the body, which can cause reinforcing of the arteries, or atherosclerosis, to stroke, kidney disease, and to heart failure.\n" +
                                "</br>" +
                                "You should follow healthy lifestyle to balance your blood pressure because it may risk your life." +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.Minimise the salt level in your diet.\n" + "</br>" +
                                "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "4.Limiting alcoholic beverages and smoking.\n" + "</br>";
                        } else {
                            var res = "less than 5000";
                            var sug = "Unfortunately, you have high blood pressure.";
                            var other = "Your blood pressure is too high and you need to seek medical help immediately. High blood pressure will directly threaten your life, and you should also control your salt intake, because according to the recommendations of the World Health Organization, the daily salt intake of adults should be less than 5000mg.\n" +
                                "</br>" +
                                "Here are also some useful suggestions for you:" +
                                "</br>" +
                                "1.Regular Monitoring of blood pressure.\n" + "</br>" +
                                "2.Minimise the salt level in your diet.\n" + "</br>" +
                                "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                                "4.Limiting alcoholic beverages and smoking.\n" + "</br>" ;
                        }


                    }else if(ysalt<2000){
                        //When the user does not have high blood pressure problems, and intake of too little salt
                        var res = Number(ysalt)+500;
                        var sug = "Congratulations, you are in good health!";
                        var other = "Your blood pressure is relatively Good.You can maintain your eating habits.But you eat too little salt" +
                            "</br>" +
                            "Your current salt intake is far below the normal standard. It will not cause your blood pressure to rise." +
                            " Long-term lack of salt may have other effects on your health.\n" + "</br>"+
                            "You can try to increase the salt intake properly under the premise of not more than 5000mg";
                    }else if (ysalt>5000) {
                        //When the user does not have high blood pressure, but takes too much salt
                        var res = "less than 5000";
                        var sug = "Congratulations, you are in good health!";
                        var other = "Your blood pressure is relatively Good.But you eat too much salt. You should control your salt intake under 5000mg" +
                            "</br>" +
                            "Your lifestyle is very well balanced, and you can continue doing the same with following few healthy factors.\n" + "</br>" +
                            "1.Maintaining the salt level in your diet.\n" + "</br>" +
                            "2.Limiting alcoholic beverages.\n" + "</br>" +
                            "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                            "4.Eating more healthy food to lower the risk of high blood pressure.\n" + "</br>" + "</br>"
                    }else {
                        //When the user does not have high blood pressure and the salt intake is within a reasonable range
                        var res = ysalt;
                        var sug = "Congratulations, you are in good health!";
                        var other = "Your blood pressure is relatively Good.You can maintain your eating habits." +
                            "</br>" +
                            "Your lifestyle is very well balanced, and you can continue doing the same with following few healthy factors.\n" + "</br>" +
                            "1.Maintaining the salt level in your diet.\n" + "</br>" +
                            "2.Limiting alcoholic beverages.\n" + "</br>" +
                            "3.To exercise regularly to have a good flow of blood.\n" + "</br>" +
                            "4.Eating more healthy food to lower the risk of high blood pressure.\n" + "</br>" + "</br>"
                    }
                    document.getElementById("feedback").innerHTML = "<p style=\'font-family: Arial;text-align: center;font-size: 20px\'>Your report</p>" + "<br/>" +
                        "<p style='text-align: left;font-size: 17px'>Your BP situation:   " + sug + "<br/>" +
                        "<p style='text-align: left'>Your Diastolic blood pressure of today：  " + dbp + " mm Hg" + "<br/>" +
                        "<p style='text-align: left'>Systolic blood pressure of today：  " + sbp + " mm Hg" + "<br/>" +
                        "<p style='text-align: left'>The salt you ate yesterday：  " + ysalt + " mg" + "<br/>" +
                        "<p style='text-align: left'>Suggest salt intake for today:  " + res + " mg" + "<br/>" +
                        "<p style='text-align: left'> Other suggestion:" + other;

                }
            }


            function checkInt(n, min, max) {
                //This function is to check whether the diastolic pressure entered by the user is within the range of normal human blood pressure,
                // thereby eliminating abnormal input
                if (n < min && n > 0) {
                    alert("The value you entered is too small.\n" +
                        "Please input the correct value between 50-250.")
                    var dbp = document.getElementById("dbp");
                    dbp.style.backgroundColor = "rgba(223,36,29,0.67)";
                } else if (n > max) {
                    alert("The value you entered is too large. \n" +
                        "Please input the correct value between 50-250.\n")
                    var dbp = document.getElementById("dbp");
                    dbp.style.backgroundColor = "rgba(223,36,29,0.67)";
                } else {
                    var dbp = document.getElementById("dbp");
                    dbp.style.backgroundColor = "rgba(255,252,249,0)";
                }
            }

            function checkInt1(n, min, max) {
                //This function is to check whether the systolic blood pressure input by the user is within the range of normal human blood pressure,
                // thereby eliminating abnormal input
                if (n < min && n > 0) {
                    alert("The value you entered is too small. \n" +
                        "Please input the correct value between 30-180.\n")
                    var sbp = document.getElementById("sbp");
                    sbp.style.backgroundColor = "rgba(223,36,29,0.67)";
                } else if (n > max) {
                    alert("The value you entered is too large. \n" +
                        "Please input the correct value between 30-180.\n")
                    var sbp = document.getElementById("sbp");
                    sbp.style.backgroundColor = "rgba(223,36,29,0.67)";
                } else {
                    var sbp = document.getElementById("sbp");
                    sbp.style.backgroundColor = "rgba(255, 252, 249, 0.0)";
                }
            }

            function checkInt2(n, max) {
                if (n < 0) {
                    alert("You can not eat negative salt!")
                    var ysalt = document.getElementById("ysalt");
                    ysalt.style.backgroundColor = "rgba(223,36,29,0.67)";
                } else if (n > max) {
                    alert("The value of salt intake you entered is too large. \n")
                    var ysalt = document.getElementById("ysalt");
                    ysalt.style.backgroundColor = "rgba(223,36,29,0.67)";
                } else {
                    var ysalt = document.getElementById("ysalt");
                    ysalt.style.backgroundColor = "rgba(255, 252, 249, 0.0)";
                }

            }
        </script>
</body>
</html>
